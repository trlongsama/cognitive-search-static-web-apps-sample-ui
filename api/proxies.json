{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "searchEndpoint": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/api/search"
            },
            "backendUri": "https://%SearchServiceName%.search.windows.net/indexes/%SearchIndexName%/docs",
            "requestOverrides": {
                "backend.request.headers.api-key": "%SearchApiKey%",
                "backend.request.querystring.api-version": "2019-05-06"
            }
        },
        "lookupEndpoint": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/api/lookup/{key}"
            },
            "backendUri": "https://%SearchServiceName%.search.windows.net/indexes/%SearchIndexName%/docs/{key}",
            "requestOverrides": {
                "backend.request.headers.api-key": "%SearchApiKey%",
                "backend.request.querystring.api-version": "2019-05-06"
            }
        },
        "autocompleteEndpoint": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/api/autocomplete"
            },
            "backendUri": "https://%SearchServiceName%.search.windows.net/indexes/%SearchIndexName%/docs/autocomplete",
            "requestOverrides": {
                "backend.request.headers.api-key": "%SearchApiKey%",
                "backend.request.querystring.api-version": "2019-05-06"
            }
        },
        "configScript": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/api/config-script"
            },
            "responseOverrides": {
                "response.body": "const ServerSideConfig = { SearchServiceName: '%SearchServiceName%', SearchIndexName: '%SearchIndexName%', AzureMapSubscriptionKey: '%AzureMapSubscriptionKey%', CognitiveSearchKeyField: '%CognitiveSearchKeyField%', CognitiveSearchNameField: '%CognitiveSearchNameField%', CognitiveSearchDocLink: '%CognitiveSearchDocLink%', CognitiveSearchGeoLocationField: '%CognitiveSearchGeoLocationField%', CognitiveSearchOtherFields: '%CognitiveSearchOtherFields%', CognitiveSearchTranscriptFields: '%CognitiveSearchTranscriptFields%', CognitiveSearchFacetFields: '%CognitiveSearchFacetFields%', CognitiveSearchSuggesterName: '%CognitiveSearchSuggesterName%' }",
                "response.headers.Content-Type": "application/javascript; charset=UTF-8"
            }
        }
    }
}